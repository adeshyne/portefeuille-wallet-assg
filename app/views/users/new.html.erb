            <%= form_for @user do |f| %>
			<%=f.label :first_name %>
			<%=f.text_field :first_name %>
             
			
			     <%=f.label :last_name %>
			<%=f.text_field :last_name%>

			

			     <%=f.label :email %>
			<%=f.text_field :email %>
             
             

			     <%=f.label :password %>
			<%=f.text_field :password %>
             
             

			     <%=f.label :phone %>
			<%=f.text_field :phone%>
             
             

			     <%=f.label :balance %>
			<%=f.text_field :balance%>
             
       

			<%= f.submit :create %>
			<% end %>